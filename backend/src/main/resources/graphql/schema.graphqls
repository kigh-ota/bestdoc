#scalar Date

type Note {
    id: ID!
    title: String!
    text: String!
#    createdAt: Date!
#    updatedAt: Date!
}

type Query {
    allNotes: [Note!]!
    getNote(id: ID!): Note!
}

type Mutation {
    addNote(title: String!, text: String!): ID!
}

schema {
    query: Query
    mutation: Mutation
}
