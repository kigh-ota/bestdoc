type Note {
    id: ID!
    title: String!
    text: String!
    createdAt: String!
    updatedAt: String!
}

type Query {
    allNotes: [Note!]!
    getNote(id: ID!): Note!
}

type Mutation {
    addNote(title: String!, text: String!): Note!
    updateNote(id: ID!, title: String!, text: String!): Note!
    deleteNote(id: ID!): ID!
}

schema {
    query: Query
    mutation: Mutation
}
